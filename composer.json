{
    "name": "corneltek/cliframework",
    "homepage": "http://github.com/c9s/CLIFramework",
    "description": "Command-line framework for PHP",
    "keywords": [
        "command",
        "command-line",
        "completion",
        "zsh",
        "framework",
        "getopt"
    ],
    "require": {
        "php": ">=7.2",
        "corneltek/getoptionkit": "^2",
        "corneltek/class-template": "^2",
        "corneltek/universal": ">= 1.4",
        "corneltek/codegen": "^2",
        "pimple/pimple": "*",
        "symfony/finder": "^2.7",
        "symfony/class-loader": "^2.7"
    },
    "require-dev": {
        "corneltek/phpunit-testmore": "dev-master",
        "satooshi/php-coveralls": "^1",
        "phpunit/phpunit": "^8.5 || ^9.5",
        "ext-intl": "*"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Yo-An Lin",
            "email": "cornelius.howl@gmail.com",
            "homepage": "http://c9s.me"
        }
    ],
    "autoload": {
        "psr-4": {
            "CLIFramework\\": "src/"
        }
    },
    "extra": { "branch-alias": { "dev-master": "3.0.x-dev" } }
}
