{
    "name": "corneltek/cliframework",
    "homepage": "http://github.com/c9s/CLIFramework",
    "description": "Command-line framework for PHP",
    "keywords": [
        "command",
        "command-line",
        "completion",
        "zsh",
        "framework",
        "getopt"
    ],
    "require": {
        "php": ">=5.3.0",
        "corneltek/getoptionkit": "~2.5.4",
        "corneltek/class-template": "^3.0.0",
        "corneltek/universal": "^1.7",
        "corneltek/codegen": "^3.1.0",
        "pimple/pimple": "*",
        "symfony/finder": "~2.8|~3.0|~3.2",
        "symfony/class-loader": "~2.8|~3.0|~3.2"
    },
    "require-dev": {
        "corneltek/phpunit-testmore": "dev-master",
        "satooshi/php-coveralls": "^1"
    },
    "license": "MIT",
    "authors": [
        { "name": "Yo-An Lin", "email": "yoanlin93@gmail.com" }
    ],
    "autoload": {
        "psr-4": {
            "CLIFramework\\": "src/"
        }
    },
    "extra": { "branch-alias": { "dev-master": "4.0.x-dev" } }
}
